<script>    
     function ShowPromoHeaderBar(content, code) {
        //jQuery lib has not loaded 
        if (!$('#promoHeaderBar')) {
            window.ShowPromoHeaderBar_content = content;
            window.ShowPromoHeaderBar_code = code;
            window.ShowPromoHeaderBar_timer = setInterval(function () {
                if ($('#promoHeaderBar')) {
                    clearInterval(window.ShowPromoHeaderBar_timer);
                    $('#promoHeaderBar').attr('data', window.ShowPromoHeaderBar_code);
                    $('#promoHeaderBar div').text(window.ShowPromoHeaderBar_content);
                    $('#promoHeaderBar').show();
                }
            }, 500);
        } else {
            $('#promoHeaderBar').attr('data', code);
            $('#promoHeaderBar div').text(content);
            $('#promoHeaderBar').show();
        }
    } 
</script> <!-- Page promo Header Bar End -->

