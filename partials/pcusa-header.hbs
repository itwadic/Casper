<script>
    var PageHeader = function (ui) {
        var self = this;
        this.ui = ui;
        this.init();
        $('.pageHeader-loginStatus').click(function (event) {
            if (self.isLogin) {
                event.preventDefault();
                event.stopPropagation();
                //window.location.href="/Shopping/Login.aspx"
               // window.location.reload();
            }

        });
    };
    PageHeader.prototype.init = function () {
        var self = this;
        this.loginCheck();
        this.loadCartInfo();

        $('.pageHeader-loginStatus').click(function (event) {
            if (self.isLogin) {
                event.preventDefault();
                event.stopPropagation();
                PCUSAService.callPricingToolServiceAPI('Logout', "", function (result) {
                    //window.location.reload();
                }, function (error) {
                });

                return false;
            } else {

                return true;
            }

        });
    }
    PageHeader.prototype.loadCartInfo = function () {
        return;
        var self = this;
        PCUSAService.callWebAPIServiceAPI('GetCartInfo', "", function (result) {

            if (result.CartItems > 0) {
                if (self.ui.find('.pageHeader-viewCart').hasClass('pageHeader-viewCart-mobile')) {
                    //self.ui.find('.pageHeader-viewCart').text(result.CartItems);
                } else {
                    self.ui.find('.pageHeader-viewCart').text('View Cart (' + result.CartItems + ')');
                }
            }
        }, function (error) {

        });

    }
    PageHeader.prototype.loginCheck = function () {
        var self = this;
        var loginItem = localStorage.getItem("islogin")=="true" ? true:false;
        self.isLogin = loginItem;
        if (self.isLogin) {
            self.ui.find('.pageHeader-loginStatus').text('Logout');
           // window.location.reload();
            self.ui.find('.pageHeader-loginStatus').attr('href', '/Shopping/Login.aspx');
            
        } else {
            self.ui.find('.pageHeader-loginStatus').text('Login');
            self.ui.find('.pageHeader-loginStatus').attr('href', '/Shopping/Login.aspx');
        }
        // added for change the live chat button image
        var intervalIndex = 0;
        var cbiInt = setInterval(function () {
            if ($("#wsa_embed") && $("#wsa_embed").css("background-image") && $("#wsa_embed").css("background-image") != "") {
                if (self.isLogin)
                    $("#wsa_embed").css("background-image",
                        "url(https://images.websitealive.com/images/hosted/upload/2016-04-19_1461030897199_yjqhk.png)");
                else
                    $("#wsa_embed").css("background-image",
                        "url(https://images.websitealive.com/images/hosted/upload/2017-04-21_1492754324431_bittnhcxr.png)");
                if (intervalIndex++ > 40)
                    clearInterval(cbiInt);
            }
        }, 100);
    } // End function prototype.loginCheck
    $(function () {
        new PageHeader($('.pageHeader'));
        $('#custom_gcs_input').keydown(function (e) {
            var theEvent = e || window.event;
            var code = theEvent.keyCode || theEvent.which || theEvent.charCode;
            if (code == 13) {
                $('input.gsc-input').val($('#custom_gcs_input').val());
                $('button.gsc-search-button').trigger('click');
                return false;
            }
            return true;
        });

        initMobileNav();
    });

    function initMobileNav() {
        $('.mobile-navigation-nav-catalog > .card-body a').click(function () {
            window.topMobileNav_CurrentClickItem = $(this).attr('aria-controls');
            $('.mobile-navigation-nav-catalog > .card-body.active').removeClass('active');

            $('.mobile-navigation-nav-catalog.no-bottom-border').removeClass('no-bottom-border');
            $(this).closest('.mobile-navigation-nav-catalog').prev().addClass('no-bottom-border')

            if ($(this).attr('aria-expanded') != 'true') {
                $(this).parent().addClass('active');
            }
            //$(this).parent().addClass('active');
            //Togger other menu
            $('.mobile-navigation-nav-catalog > .card-body a').each(function (index, item) {
                if ($(item).attr('aria-controls') == window.topMobileNav_CurrentClickItem) {
                    return true;
                }
                $(item).parent().removeClass('active');
                $(item).attr('aria-expanded', 'false');
                $(item).parent().next().removeClass('show');
                //$(item).trigger('click');
            });
        });
        //Disable document body scroll after menu open
        $('#mobile-hamburger').on('touchend', function () {
            $('body').css('overflow', 'hidden');
        });
        $('#mobile-hamburger-close').on('touchend', function () {
            $('body').css('overflow', 'initial');
        })
    }

</script>
