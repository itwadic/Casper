		<script>
			$(function () {

				if (document.getElementById("pricing-tool")) {
					document.getElementById('instant-quote').style.display = "block";
				}
				else {
					document.getElementById('instant-quote').style.display = "none";
				}

				$('#instant-quote .instant-quote').click(function () {
					var target = $("#pricing-tool");
					// console.log(target);
					$('html, body').animate({
						scrollTop: $(target).offset().top - 90
					}, 500);
					return false;
				});
				// Scroll to top.
				var offset = 250;
				var duration = 300;
				jQuery(window).scroll(function () {
					if (jQuery(this).scrollTop() > offset) {
						jQuery('.back-to-top').fadeIn(duration);
					} else {
						jQuery('.back-to-top').fadeOut(duration);
					}
				});

				jQuery('.back-to-top').on('click', function (event) {
					event.preventDefault();
					jQuery('html, body').animate({ scrollTop: 0 }, duration);
					return false;
				});

				$('.post-content  img').liteBox();
			});
		</script>
